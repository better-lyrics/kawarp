{
  "name": "@kawarp/react",
  "version": "1.1.0",
  "description": "React component for Kawarp fluid animated backgrounds",
  "keywords": [
    "react",
    "webgl",
    "blur",
    "kawase",
    "animation",
    "background",
    "canvas",
    "component"
  ],
  "license": "AGPL-3.0",
  "author": "Better Lyrics",
  "homepage": "https://github.com/better-lyrics/kawarp",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/better-lyrics/kawarp.git",
    "directory": "packages/react"
  },
  "bugs": {
    "url": "https://github.com/better-lyrics/kawarp/issues"
  },
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch --preserveWatchOutput",
    "check-types": "tsc --noEmit"
  },
  "dependencies": {
    "@kawarp/core": "workspace:*"
  },
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0"
  },
  "devDependencies": {
    "@types/react": "^19.0.0",
    "typescript": "^5.0.0"
  }
}
